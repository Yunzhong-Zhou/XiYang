<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <rotate
            android:fromDegrees="45"
            android:pivotX="85%"
            android:pivotY="135%">
            <shape android:shape="rectangle">
                <size
                    android:width="18dp"
                    android:height="18dp" />
                <solid android:color="#00D59D" />
            </shape>
        </rotate>
    </item>
</layer-list>